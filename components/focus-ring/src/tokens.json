{
  "utrecht": {
    "focus": {
      "background-color": {
        "$extensions": {
          "nl.nldesignsystem.css.property-syntax": "<color>",
          "nl.nldesignsystem.figma-implementation": true
        },
        "type": "color"
      },
      "color": {
        "$extensions": {
          "nl.nldesignsystem.css.property-syntax": "<color>",
          "nl.nldesignsystem.figma-implementation": true
        },
        "type": "color"
      },
      "outline-color": {
        "$extensions": {
          "nl.nldesignsystem.css.property-syntax": "<color>",
          "nl.nldesignsystem.figma-implementation": true
        },
        "style": "color"
      },
      "outline-offset": {
        "$extensions": {
          "nl.nldesignsystem.css.property-syntax": "<length>",
          "nl.nldesignsystem.figma-implementation": false
        },
        "type": "other"
      },
      "outline-style": {
        "$extensions": {
          "nl.nldesignsystem.css.property-syntax": ["dotted", "none", "solid"],
          "nl.nldesignsystem.figma-implementation": false
        },
        "type": "other"
      },
      "outline-width": {
        "$extensions": {
          "nl.nldesignsystem.css.property-syntax": "<length>",
          "nl.nldesignsystem.figma-implementation": true
        },
        "style": "borderWidth"
      },
      "inverse": {
        "outline-color": {
          "$extensions": {
            "nl.nldesignsystem.css.property-syntax": "<color>",
            "nl.nldesignsystem.figma-implementation": true
          },
          "style": "color"
        }
      }
    }
  }
}
