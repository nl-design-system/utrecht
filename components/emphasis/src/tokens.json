{
  "utrecht": {
    "emphasis": {
      "stressed": {
        "font-style": {
          "$extensions": {
            "nl.nldesignsystem.css.property": {
              "syntax": ["inherit", "italic", "normal"],
              "inherits": true
            },
            "nl.nldesignsystem.figma.supports-token": false
          },
          "type": "other"
        }
      },
      "strong": {
        "font-weight": {
          "$extensions": {
            "nl.nldesignsystem.css.property": {
              "syntax": "<number>",
              "inherits": true
            },
            "nl.nldesignsystem.figma.supports-token": false
          },
          "type": "other"
        }
      }
    }
  }
}
