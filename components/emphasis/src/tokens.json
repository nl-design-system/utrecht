{
  "utrecht": {
    "emphasis": {
      "stressed": {
        "font-style": {
          "$extensions": {
            "nl.nldesignsystem.css.property-syntax": ["inherit", "italic", "normal"],
            "nl.nldesignsystem.figma-implementation": false
          },
          "type": "other"
        }
      },
      "strong": {
        "font-weight": {
          "$extensions": {
            "nl.nldesignsystem.css.property-syntax": "<number>",
            "nl.nldesignsystem.figma-implementation": false
          },
          "type": "fontWeights"
        }
      }
    }
  }
}
