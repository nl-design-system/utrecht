{
  "utrecht": {
    "emphasis": {
      "stressed": {
        "font-style": {
          "$extensions": {
            "nl.nldesignsystem.css.property": {
              "syntax": "inherit | italic | normal",
              "inherits": true
            }
          }
        }
      },
      "strong": {
        "font-weight": {
          "$extensions": {
            "nl.nldesignsystem.css.property": {
              "syntax": "<number>",
              "inherits": true
            }
          }
        }
      }
    }
  }
}
