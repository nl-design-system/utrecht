{
  "utrecht": {
    "breadcrumb-nav": {
      "min-block-size": {
        "$extensions": {
          "nl.nldesignsystem.css.property-syntax": "<length>",
          "nl.nldesignsystem.figma-implementation": false
        },
        "type": "spacing"
      },
      "font-family": {
        "$extensions": {
          "nl.nldesignsystem.css.property-syntax": "*",
          "nl.nldesignsystem.figma-implementation": false
        },
        "type": "fontFamilies"
      },
      "font-size": {
        "$extensions": {
          "nl.nldesignsystem.css.property-syntax": "<length>",
          "nl.nldesignsystem.figma-implementation": false
        },
        "type": "fontSizes"
      },
      "line-height": {
        "$extensions": {
          "nl.nldesignsystem.css.property-syntax": ["<length>", "<number>"],
          "nl.nldesignsystem.figma-implementation": false
        },
        "type": "lineHeights"
      },
      "text-decoration": {
        "$extensions": {
          "nl.nldesignsystem.css.property-syntax": ["inherit", "none", "underline"],
          "nl.nldesignsystem.fallback": ["utrecht.breadrumb-nav.link.text-decoration"],
          "nl.nldesignsystem.figma-implementation": false
        },
        "type": "textDecoration"
      },
      "text-transform": {
        "$extensions": {
          "nl.nldesignsystem.css.property-syntax": ["inherit", "uppercase"],
          "nl.nldesignsystem.figma-implementation": false
        },
        "type": "textCase"
      },
      "item": {
        "padding-block-start": {
          "$extensions": {
            "nl.nldesignsystem.css.property-syntax": "<length>",
            "nl.nldesignsystem.figma-implementation": false
          },
          "type": "spacing"
        },
        "padding-block-end": {
          "$extensions": {
            "nl.nldesignsystem.css.property-syntax": "<length>",
            "nl.nldesignsystem.figma-implementation": false
          },
          "type": "spacing"
        },
        "padding-inline-end": {
          "$extensions": {
            "nl.nldesignsystem.css.property-syntax": "<length>",
            "nl.nldesignsystem.figma-implementation": false
          },
          "type": "spacing"
        },
        "padding-inline-start": {
          "$extensions": {
            "nl.nldesignsystem.css.property-syntax": "<length>",
            "nl.nldesignsystem.figma-implementation": false
          },
          "type": "spacing"
        }
      },
      "link": {
        "color": {
          "$extensions": {
            "nl.nldesignsystem.css.property-syntax": "<color>",
            "nl.nldesignsystem.figma-implementation": false
          },
          "type": "color"
        },
        "focus": {
          "background-color": {
            "$extensions": {
              "nl.nldesignsystem.css.property-syntax": "<color>",
              "nl.nldesignsystem.figma-implementation": false
            },
            "type": "color"
          },
          "color": {
            "$extensions": {
              "nl.nldesignsystem.css.property-syntax": "<color>",
              "nl.nldesignsystem.fallback": ["utrecht.breadrumb-nav.link.color"],
              "nl.nldesignsystem.figma-implementation": false
            },
            "type": "color"
          },
          "text-decoration": {
            "$extensions": {
              "nl.nldesignsystem.css.property-syntax": ["inherit", "none", "underline"],
              "nl.nldesignsystem.fallback": ["utrecht.breadrumb-nav.link.text-decoration"],
              "nl.nldesignsystem.figma-implementation": false
            },
            "type": "textDecoration"
          }
        },
        "hover": {
          "color": {
            "$extensions": {
              "nl.nldesignsystem.css.property-syntax": "<color>",
              "nl.nldesignsystem.fallback": ["utrecht.breadrumb-nav.link.color"],
              "nl.nldesignsystem.figma-implementation": false
            },
            "type": "color"
          },
          "text-decoration": {
            "$extensions": {
              "nl.nldesignsystem.css.property-syntax": ["inherit", "none", "underline"],
              "nl.nldesignsystem.fallback": ["utrecht.breadrumb-nav.link.text-decoration"],
              "nl.nldesignsystem.figma-implementation": false
            },
            "type": "textDecoration"
          }
        },
        "current": {
          "font-weight": {
            "$extensions": {
              "nl.nldesignsystem.css.property-syntax": "<number>",
              "nl.nldesignsystem.figma-implementation": false
            },
            "type": "fontWeights"
          }
        },
        "disabled": {
          "color": {
            "$extensions": {
              "nl.nldesignsystem.css.property-syntax": "<color>",
              "nl.nldesignsystem.figma-implementation": false
            },
            "type": "color"
          }
        }
      },
      "separator": {
        "color": {
          "$extensions": {
            "nl.nldesignsystem.css.property-syntax": "<color>",
            "nl.nldesignsystem.figma-implementation": false
          },
          "type": "color"
        },
        "icon": {
          "size": {
            "$extensions": {
              "nl.nldesignsystem.css.property-syntax": "<length>",
              "nl.nldesignsystem.figma-implementation": false
            },
            "type": "sizing"
          }
        }
      },
      "arrows": {
        "link": {
          "background-color": {
            "$extensions": {
              "nl.nldesignsystem.css.property-syntax": "<color>",
              "nl.nldesignsystem.figma-implementation": false
            },
            "type": "color"
          },
          "focus": {
            "background-color": {
              "$extensions": {
                "nl.nldesignsystem.css.property-syntax": "<color>",
                "nl.nldesignsystem.figma-implementation": false
              },
              "type": "color"
            }
          }
        }
      }
    }
  }
}
