{
  "$schema": "../../node_modules/nx/schemas/workspace-schema.json",
  "root": "/components/icon",
  "sourceRoot": "/components/icon",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["components/icon/dist", "components/icon/tmp"],
      "options": {
        "command": "npm run build",
        "cwd": "components/icon"
      }
    },
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run clean",
        "cwd": "components/icon"
      }
    }
  }
}
