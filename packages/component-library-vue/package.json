{
  "version": "1.0.0-alpha.97",
  "author": "Community for NL Design System",
  "description": "Vue.js components for the Municipality of Utrecht based on the NL Design System architecture",
  "license": "EUPL-1.2",
  "name": "@utrecht/component-library-vue",
  "keywords": [
    "nl-design-system"
  ],
  "private": false,
  "peerDependency": {
    "@vue/core": "^3.0.0",
    "@vue/shared": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:nl-design-system/utrecht.git"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "npm-run-all lint:**",
    "lint:ts": "vue-tsc --noEmit -p tsconfig.lib.json --composite false",
    "lint:ts-jest": "vue-tsc --noEmit -p tsconfig.jest.json --composite false",
    "preview": "vite preview --port 5050",
    "test": "jest"
  },
  "main": "./dist/component-library-vue.umd.js",
  "module": "./dist/component-library-vue.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@rushstack/eslint-patch": "1.1.4",
    "@testing-library/vue": "6.6.1",
    "@types/jest": "29.0.0",
    "@types/jsdom": "16.2.15",
    "@types/node": "16.11.47",
    "@utrecht/components": "file:../../components",
    "@vitejs/plugin-vue": "2.3.3",
    "@vue/test-utils": "2.0.2",
    "@vue/tsconfig": "0.1.3",
    "@vue/vue3-jest": "29.0.0",
    "jest": "29.0.1",
    "jest-environment-jsdom": "29.0.1",
    "jsdom": "19.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "sass": "1.54.0",
    "ts-jest": "29.0.0-next.0",
    "typescript": "4.8.2",
    "vite": "2.9.14",
    "vite-plugin-dts": "1.4.1",
    "vue": "3.2.37",
    "vue-router": "4.1.3",
    "vue-tsc": "0.39.4"
  }
}
