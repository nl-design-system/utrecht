{
  "version": "1.0.0-alpha.229",
  "author": "Community for NL Design System",
  "description": "Vue.js web components for the Municipality of Utrecht based on the NL Design System architecture",
  "license": "EUPL-1.2",
  "name": "@utrecht/web-component-library-vue",
  "keywords": [
    "nl-design-system"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:nl-design-system/utrecht.git"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist/",
    "build": "vue-tsc --noEmit && vite build"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/web-component-library-vue.umd.js",
  "module": "./dist/web-component-library-vue.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/web-component-library-vue.es.js",
      "require": "./dist/web-component-library-vue.umd.js"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "3.0.1",
    "rimraf": "3.0.2",
    "typescript": "4.1.6 <4.2",
    "vite": "3.0.4",
    "vite-plugin-dts": "1.4.1",
    "vue": "3.2.37",
    "vue-tsc": "0.39.4"
  },
  "dependencies": {
    "@utrecht/web-component-library-stencil": "file:../web-component-library-stencil"
  }
}
