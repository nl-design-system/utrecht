    "@storybook/addon-docs": "6.3.12",
    "@storybook/addon-viewport": "6.3.12",
    "@storybook/builder-webpack5": "6.3.12",
    "@storybook/cli": "6.3.12",
    "@storybook/html": "6.3.12",
    "@storybook/manager-webpack5": "6.3.12",
    "@storybook/preset-scss": "1.0.3",
