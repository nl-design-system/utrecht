{
  "$schema": "../../node_modules/nx/schemas/workspace-schema.json",
  "root": "packages/storybook-vue",
  "sourceRoot": "packages/storybook-vue",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["packages/storybook-vue/dist"],
      "options": {
        "command": "npm run build",
        "cwd": "packages/storybook-vue"
      },
      "dependsOn": ["^build"]
    },
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run clean",
        "cwd": "packages/storybook-vue"
      }
    },
    "start": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run start",
        "cwd": "packages/storybook-vue"
      },
      "dependsOn": ["^storybook"]
    },
    "storybook": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run storybook",
        "cwd": "packages/storybook-vue"
      }
    }
  },
  "implicitDependencies": ["component-library-vue"]
}
