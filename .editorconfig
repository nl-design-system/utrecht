root = true

[*.{css,js,json,jsx,html,md,mdx,scss,svg,ts,tsx}]
charset = UTF-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
max_line_length = 120
trim_trailing_whitespace = true

[*ignore]
charset = UTF-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
